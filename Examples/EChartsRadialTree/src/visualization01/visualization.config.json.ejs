{
  "id": "1a4c06c4-b019-40dc-b248-7fa386a1553a",
  "name": "ECharts Radial Tree",
  "description": "ECharts Custom Radial Tree Chart Visualization",
  "version": "<%= package.version %>",
  "supportedVersions": "1.0",
  "moodVersion": "16.0.82",
  "entry": {
    "file": "visualization.js",
    "function": "vis.visualization"
  },
  "supportedBrowsers": "Chrome, Edge, Firefox",
  "inputs": [
    {
      "name": "maxDepth",
      "displayName": "Maximum Tree Depth",
      "type": "Number"
    }
  ],
  "outputs": [
    {
      "name": "hoverNode",
      "displayName": "Node",
      "type": "elements"
    }
  ],
  "style": {
    "backgroundColor": "rgba(255, 255, 255, 1)",
    "series": {
      "top": "18%",
      "bottom": "14%",
      "layout": "radial",
      "orient": "LR",
      "showTooltip": false,
      "symbol": "emptyCircle",
      "symbolSize": 7,
      "itemStyle": {
        "color": "rgba(180, 180, 180, 0.2)"
      },
      "label": {
        "show": true,
        "fontSize": 12,
        "rotate": 0
      },
      "leafLabel": {
        "show": true,
        "fontSize": 12
      },
      "emphasis": {
          "focus": "descendant"
      }
    }
  }
}