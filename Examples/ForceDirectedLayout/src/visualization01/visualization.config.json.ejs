{
  "id": "6db14964-d67f-48aa-a8c1-34d50e5431c2",
  "version": "<%= package.version %>",
  "name": "Force Directed Layout",
  "description": "Network chart using D3 Force Directed Layout",
  "supportedVersions": "0",
  "moodVersion": "16.0.082",
  "entry": {
    "file": "visualization.js",
    "function": "vis.createForceLayout"
  },
  "supportedBrowsers": "Chrome, Edge, FireFox",
  "inputs": [
    {
      "name": "showLabels",
      "displayName": "Show node labels",
      "type": "Number"
    },
    {
      "name": "nodeForce",
      "displayName": "Force between nodes",
      "type": "Number"
    },
    {
      "name": "gravityStrength",
      "displayName": "Strength of Gravity",
      "type": "Number"
    },
    {
      "name": "linkStrength",
      "displayName": "Elastic strength of links",
      "type": "Number"
    },
    {
      "name": "linkLength",
      "displayName": "Length of links",
      "type": "Number"
    }
  ],
  "outputs": [
    {
      "name": "hoverNode",
      "displayName": "Node",
      "type": "elements"
    }
  ],
  "actions": {
    "Node Click": {
      "description": "A node representing an element has been clicked.",
      "title": "Node"
    }
  },
  "style": {
    "URL":"Style.css",
    "JSON": {
      "Node Minimum Radius": 5,
      "Node Maximum Radius": 100,
      "Unlinked Node Force Strength": 10,
      "Unlinked Node Cluster Repositioning Strength": 0.1,
      "Unlinked Node Cluster x": 100,
      "Link Colour": "#000",
      "Link Arrow": false,
      "Curved Links": false,
      "Ignore Unknown Nodes": true,
      "Show Label Node Separation": 100,
      "Disable Pan and Zoom": false,
      "Zoom Minimum": 0.5,
      "Zoom Maximum": 5,
      "Pan Extent": 1.0
    }
  }
}
