{
  "id": "f1ff54e2-125d-4c3f-a4af-ff672bf40939",
  "name": "Process Flow Visualisation",
  "description": "Visualisation of a Process Flow in swimlanes",
  "version": "<%= package.version %>",
  "supportedVersions": "2.6",
  "moodVersion": "16.0.082",
  "dynamicData": false,
  "canOverflow":  true,
  "entry": {
    "file": "visualization.js",
    "function": "vis.visualization"
  },
  "supportedBrowsers": "Chrome, Edge, Firefox",
  "inputs": [
    {
      "name": "highlightNode",
      "displayName": "Highlight Node",
      "type": "Elements"
    }
  ],
  "style": {
    "URL": "visualization01/process-flow.css",
    "JSON": {
      "verticalSwimlanes": true,
      "minimumSwimlaneHeight": 170,
      "horizontalStepsAllowed": true,
      "horizontalDecisionsAllowed": true,
      "renderProcessHeader": false,
      "renderSwimlaneWatermarks": false,
      "gridSize": 10,
      "verticalStepSeparation": 50,
      "verticalIOSeparation": 10,
      "stepStandoff": 10,
      "ioStandoff": 10,
      "linkLabelStandoff": 70,
      "processHeaderHeight": 20,
      "swimlaneWatermarkSpacing": 400,
      "phaseLabelWidth": 60,
      "stepGroupPadding": 15,
      "elementSizes": {
        "Process Step": {"width": 100, "height": 60},
        "Decision": {"width": 100, "height": 100},
        "Decision Large": {"width": 150, "height": 150},
        "Off Page Input": {"width": 100, "height": 60},
        "Off Page Output": {"width": 100, "height": 60},
        "Sub Process": {"width": 100, "height": 60},
        "Process": {"width": 100, "height": 30},
        "Start": {"width": 100, "height": 40},
        "End": {"width": 100, "height": 40},
        "Document / Form": {"width": 100, "height": 40},
        "External Data": {"width": 100, "height": 40},
        "Database / Application": {"width": 100, "height": 40},
        "Data": {"width": 100, "height": 40},
        "Other": {"width": 100, "height": 40},
        "BPMN Start Event" : {"width": 40, "height": 40},
        "BPMN Start Event Message" : {"width": 40, "height": 40},
        "BPMN Start Event Timer" : {"width": 40, "height": 40},
        "BPMN Start Event Error" : {"width": 40, "height": 40},
        "BPMN Intermediate Event" : {"width": 40, "height": 40},
        "BPMN Intermediate Event Message" : {"width": 40, "height": 40},
        "BPMN Intermediate Event Timer" : {"width": 40, "height": 40},
        "BPMN Intermediate Event Error" : {"width": 40, "height": 40},
        "BPMN End Event" : {"width": 40, "height": 40},
        "BPMN End Event Message" : {"width": 40, "height": 40},
        "BPMN End Event Timer" : {"width": 40, "height": 40},
        "BPMN End Event Error" : {"width": 40, "height": 40},
        "BPMN Exclusive Gateway" : {"width": 40, "height": 40},
        "BPMN Parallel Gateway" : {"width": 40, "height": 40},
        "BPMN Inclusive Gateway" : {"width": 40, "height": 40},
        "BPMN Data Object" : {"width": 40, "height": 60},
        "BPMN Data Object Input" : {"width": 40, "height": 60},
        "BPMN Data Object Output" : {"width": 40, "height": 60},
        "BPMN Data Storage" : {"width": 40, "height": 40}
      },
      "inputSwimlaneLabel": "Inputs",
      "outputSwimlaneLabel": "Outputs",
      "disableIOSwimlanes": false,
      "maxFlowLabelSize": {
        "width": 100,
        "height": 60
      },
      "router": {
        "coincidentLineSpace": 3,
        "targetTolerance": 1,
        "jumpOverOnHorizontalLines": true
      }
    }
  },
  "actions": {
    "Process Step Click": {
      "description": "A process step has been clicked",
      "title": "Step Click"
    },
    "Process I/O Click": {
      "description": "An input or output element has been clicked",
      "title": "I/O Click"
    },
    "Process Click": {
      "description": "A process node has been clicked",
      "title": "Process Click"
    },
    "Phase Click": {
      "description": "A phase has been clicked",
      "title": "Phase Click"
    },
    "Actor Click": {
      "description": "An actor (process owner) has been clicked",
      "title": "Actor Click"
    },
    "Link Click": {
      "description": "An process step flow link has been clicked",
      "title": "Link Click"
    },
    "I/O Link Click": {
      "description": "An input/output link has been clicked",
      "title": "I/O Link Click"
    },
    "Activity Group Click": {
      "description": "An activity group has been clicked",
      "title": "Activity Group Click"
    },
    "Non-navigable Node Click": {
      "description": "A non-navigable node has been clicked",
      "title": "Non-Navigable Click"
    },
    "Test Navigation": {
      "description": "Dummy action to be configured with just a Navigation action to test navigability",
      "title": "Test Navigation"
    }
  }
}