{
  "version": "<%= package.version %>",
  "id": "47634fff-3181-4e4e-8d78-000000000007",
  "name": "Stretched Chord",
  "description": "A chord diagram stretched into a more circular looking sankey-like flow chart",
  "supportedVersions": "1",
  "moodVersion": "16.0.076",
  "supportedBrowsers": "Chrome, Edge, Firefox",
  "dynamicData": true,
  "entry": {
    "file": "./visualization.js",
    "function": "vis.createStretchedChord"
  },
  "style": {
    "JSON": {
      "nodeColour": "#0080F0",
      "nodeBorderColour": "#002090",
      "nodeSeparation": 5,
      "flowOpacity": 0.5,
      "minimumNodeSizePercentage": 5,
      "flowPeriod": "5s",
      "arcThickness": 20,
      "arcCentreSeparation": 100,
      "labelMargin": 220,
      "labelOffset": 10,
      "labelFontSize": 15,
      "labelFontFamily": "sans-serif",
      "headerHeight": 10,
      "footerHeight": 10
    }
  },    
  "inputs": [
  ],
  "outputs": [
    {
      "name": "hoverNode",
      "displayName": "Node Hover",
      "type": "elements"
    },
    {
      "name": "hoverLink",
      "displayName": "Link Hover",
      "type": "elements"
    }
  ],
  "actions": {
    "Source Click": {
      "description": "A source shape representing an element has been clicked.",
      "title": "Source"
      },
    "Target Click": {
      "description": "A target shape representing an element has been clicked.",
      "title": "Target"
      },
    "Chord Click": {
      "description": "A chord representing the relationship between source and target has been clicked.",
      "title": "Link"
      }
    }
}

