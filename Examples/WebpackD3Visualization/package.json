{
    "name": "scatter_plot",
    "description": "Example scatter plot chart based on D3 framework",
    "version": "1.1.28",
    "license": "MIT",
    "dependencies": {
        "d3": "7.6.1"
    },
    "devDependencies": {
        "@babel/core": "^7.22.9",
        "@babel/preset-env": "^7.22.9",
        "babel-jest": "^29.7.0",
        "babel-loader": "^8.3.0",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "~11.0.0",
        "core-js": "^3.33.0",
        "ejs": "3.1.7",
        "file-loader": "6.2.0",
        "fs": "0.0.1-security",
        "html-loader": "~4.1.0",
        "html-webpack-plugin": "5.5.0",
        "jest": "^29.6.1",
        "jest-environment-jsdom": "^29.6.1",
        "json-loader": "0.5.7",
        "standard": "^17.1.0",
        "uuid": "8.3.2",
        "webpack": "^5.76.1",
        "webpack-cli": "4.9.1",
        "webpack-dev-server": "~4.9.3",
        "webpack-merge": "5.8.0",
        "webpack-version-file-plugin": "^0.5.0",
        "zip-webpack-plugin": "4.0.1"
    },
    "scripts": {
        "generate-guids": "node generate-guids/generate-guids.js",
        "start": "webpack-dev-server  --config webpack.dev.js --open",
        "version": "webpack --config webpack.prod.js",
        "build": "npm version patch --no-git-tag-version",
        "rebuild": "webpack --config webpack.prod.js",
        "stats": "webpack --config webpack.prod.js --profile --json > stats.json",
        "check-style": "standard \"src/**/*.js\"",
        "fix-style": "standard \"src/**/*.js\" --verbose --fix",
        "test": "jest",
        "test-init": "jest --init"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
            "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
        }
    },
    "standard": {
        "ignore": [
            "**/__tests__"
        ]
    }
}
